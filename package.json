  "name": "fitness-challenge",
  "description": "Fitness Challenge Application",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": "0.10.x"
  },
  "devDependencies": {
    "babel-core": "^6.7.7",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.2.4",
    "babel-plugin-dynamic-import-webpack": "^1.0.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.0",
    "css-chunks-html-webpack-plugin": "0.0.1",
    "css-loader": "^0.23.1",
    "cssnano": "^3.10.0",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-react": "^7.1.0",
    "extract-css-chunks-webpack-plugin": "^2.0.17",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.30.1",
    "json-loader": "^0.5.4",
    "node-sass": "4.5.3",
    "pre-commit": "^1.2.2",
    "preload-webpack-plugin": "^2.0.0",
    "progress-bar-webpack-plugin": "^1.10.0",
    "raw-loader": "^0.5.1",
    "sass-lint": "^1.12.1",
    "sass-loader": "^3.2.0",
    "script-ext-html-webpack-plugin": "^1.8.8",
    "shx": "^0.2.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^3.1.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.9.1"
  },
  "scripts": {
    "clean:build": "shx rm -rf dist",
    "test": "npm run lint && SET NODE_ENV=test ./node_modules/.bin/karma start",
    "lint": "./node_modules/.bin/eslint **/*.js",
    "sass-lint": "sass-lint -v -q",
    "analyze": "npm run lint && npm run sass-lint",
    "start": "cross-env NODE_ENV=development ./node_modules/.bin/webpack-dev-server --config config/webpack.config.js",
    "start:sit": "cross-env NODE_ENV=sit ./node_modules/.bin/webpack-dev-server --config config/webpack.config.js",
    "start:uat": "cross-env NODE_ENV=uat ./node_modules/.bin/webpack-dev-server --config config/webpack.config.js",
    "build": "npm run clean:build && cross-env NODE_ENV=production ./node_modules/.bin/webpack -p --config config/webpack.config.production.js",
    "build:sit": "npm run clean:build && cross-env NODE_ENV=sit ./node_modules/.bin/webpack -p --config config/webpack.config.production.js",
    "build:uat": "npm run clean:build && cross-env NODE_ENV=uat ./node_modules/.bin/webpack -p --config config/webpack.config.production.js",
    "build:dev": "npm run clean:build && cross-env NODE_ENV=development ./node_modules/.bin/webpack -p --config config/webpack.config.production.js",
    "deploy:dev": "npm run build:dev && aws s3 sync ./dist s3://otf-fca-app-dev --profile otfDev --delete",
    "deploy:sit": "npm run build:sit && aws s3 sync ./dist s3://otf-fca-app-sit --profile otfDev --delete",
    "deploy:uat": "npm run build:uat && aws s3 sync ./dist s3://otf-fca-app-uat --delete",
    "deploy": "npm run build && aws s3 sync ./dist s3://otf-fca-app --delete",
    "preinstall": "npm i -g eslint eslint-plugin-react",
    "postdeploy:dev": "aws cloudfront create-invalidation --distribution-id E37JMN0S8F363A --paths '/*' --profile otfDev",
    "postdeploy:sit": "aws cloudfront create-invalidation --distribution-id E2E0Z7UQPPU313 --paths '/*' --profile otfDev",
    "postdeploy:uat": "aws cloudfront create-invalidation --distribution-id E3M4ORXEY1AW7U --paths '/*'",
    "postdeploy": "aws cloudfront create-invalidation --distribution-id E241B34AHNT51L --paths '/*'"
  },
  "dependencies": {
    "amazon-cognito-identity-js": "^1.28.0",
    "autosuggest-highlight": "^3.1.0",
    "aws-sdk": "^2.166.0",
    "axios": "^0.16.1",
    "bootstrap": "^3.3.6",
    "classnames": "^2.2.1",
    "d3": "^4.11.0",
    "daterangepicker": "^2.1.25",
    "downloadjs": "^1.4.7",
    "eonasdan-bootstrap-datetimepicker": "4.17.42",
    "font-awesome": "^4.5.0",
    "fusioncharts": "^3.12.0",
    "html-to-image": "^0.1.1",
    "html2canvas": "1.0.0-alpha.10",
    "immutable": "^3.8.1",
    "jquery": "^2.2.3",
    "js-quantities": "^1.7.0",
    "lodash": "^4.17.4",
    "moment": "^2.14.1",
    "node-sass": "4.5.3",
    "normalize.css": "^3.0.3",
    "prop-types": "^15.5.7",
    "react": "^16.10.0",
    "react-autosuggest": "^9.1.0",
    "react-bootstrap": "^0.31.0",
    "react-bootstrap-table": "^4.3.1",
    "react-dom": "^16.10.0",
    "react-fusioncharts": "^1.0.4",
    "react-highlight-words": "^0.8.1",
    "react-intl": "^2.4.0",
    "react-number-format": "^4.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.4",
    "react-router-redux": "^4.0.5",
    "react-s-alert": "^1.3.0",
    "react-select": "1.0.0-rc.5",
    "react-telephone-input": "^4.73.0",
    "react-transition-group": "^1.1.1",
    "redux": "^3.6.0",
    "redux-debounce": "^1.0.1",
    "redux-form": "^7.0.3",
    "redux-logger": "^3.0.6",
    "redux-module-builder": "^0.3.2",
    "redux-thunk": "^2.2.0",
    "sass-resources-loader": "^1.3.1"
  },
  "pre-commit": [
    "analyze"
  ]
}
